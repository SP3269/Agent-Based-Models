extensions [ CSV ]
globals [ data messages-total informed-before informed-after ]
turtles-own [ id manager message? messages-received signal-quality message-quality sum-message-quality mean-message-quality ]

to setup
  clear-all
  file-close-all
  reset-ticks
  reset-timer
  ifelse empty? input-file [ set data csv:from-file "./org.csv" ] [ set data csv:from-file input-file ]
  foreach data [ datapoint -> create-agent-from-datapoint datapoint ]
  print timer
  ask links [ set color 3 ]
  set messages-total 1 ;; start with 1 message org-wide, receved by all
  set informed-before 0
  set informed-after 0
  clear-all-plots
end

to load-world
  ifelse empty? input-file [ import-world "./world.csv" ] [ import-world input-file ]
end

to go
  ifelse ( informed-before = informed-after ) [ start-messaging ] [ ask turtles with [ not message? ] [ communicate ] ]
  set informed-before informed-after
  set informed-after count turtles with [ message? = true ]
  tick
end

to create-agent-from-datapoint [ nm ] ;; nm is a list [ name manager ] for a tree structure
  let one ( word first nm )
  let two ( word last nm )
  type one type " " print two ;; debug output
  if ( not any? turtles with [ id = one ] ) [
    create-turtles 1 [
      set size 3
      set shape "dot"
      set messages-received 1 ;; start with 1 message org-wide, receved by all
      set mean-message-quality 1
      set sum-message-quality 1
      set id one
      ;; set manager other turtles with [ id = linkto ]
      ;; create-links-with turtles with [ id = two ]
      ;; layout-spring turtles links 0.2 2.0 0.5
    ]
  ]
  if ( not any? turtles with [ id = two ] ) [
    create-turtles 1 [
      set size 3
      set shape "dot"
      set messages-received 1 ;; start with 1 message org-wide, receved by all
      set mean-message-quality 1
      set sum-message-quality 1
      set id two
      ;; set manager other turtles with [ id = linkto ]
      ;; create-links-with turtles with [ id = one ]
      ;; layout-spring turtles links 0.2 2.0 0.5
    ]
  ]
  ask turtles with [ id = one ] [
      create-links-with turtles with [ id = two ]
      layout-spring turtles links 0.2 2.0 0.5
  ]
  ;; tick
end

to start-messaging ;; observer procedure - resets state of messaging and injects new message with random agent
  ask turtles [
    set message? false
    set color black
  ]
  ask one-of turtles [
    set message? true
    set signal-quality 1
    set message-quality 1
    set color white
  ]
  set messages-total messages-total + 1
end

to communicate ;; turtle procedure
  let source one-of link-neighbors with [ message? ] ;; there can be only one informed neighbor in the tree structure
  if ( source != nobody ) [
    set signal-quality signal-decay-factor * ( [ signal-quality ] of source )
    if ( signal-quality > perception-threshold ) [
      set message? true
      set message-quality quality-decay-factor * ( [ message-quality ] of source )
      set sum-message-quality sum-message-quality + message-quality
      set messages-received messages-received + 1
      set mean-message-quality sum-message-quality / messages-received
      set color scale-color white ( message-quality * 50) 0 50
    ]
  ]
end
